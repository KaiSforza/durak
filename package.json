{
  "name": "durak",
  "version": "0.1.0",
  "description": "Durak card game using electron",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build:watch": "webpack --config ./webpack.config.js --watch",
    "clean": "tsc --build --clean",
    "rebuild": "npm run clean && npm run build",
    "test": "jest --coverage",
    "watch": "jest --watch",
    "prestart": "npm run build",
    "start": "electron ./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaiSforza/durak.git"
  },
  "keywords": [
    "durak",
    "cards",
    "electron"
  ],
  "author": "Alex Giokas",
  "contributors": [
    {
      "name": "Kai Giokas",
      "email": "kai@kaictl.me",
      "url": "https://github.com/KaiSforza"
    },
    {
      "name": "Panda",
      "url": "https://github.com/pnda07"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaiSforza/durak/issues"
  },
  "homepage": "https://github.com/KaiSforza/durak#readme",
  "dependencies": {
    "electron": "^10.1.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "babel-jest": "^26.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "jest": "^26.6.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "roots": [
      "./test",
      "./src"
    ],
    "testMatch": [
      "**/*.test.[jt]s"
    ]
  }
}
