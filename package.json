{
  "name": "durak",
  "version": "0.1.1",
  "description": "Durak card game using electron",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build:tsc": "tsc --build --verbose",
    "build:watch": "webpack --config ./webpack.config.js --watch",
    "build:clean": "git clean -xdf ./dist",
    "build:rebuild": "npm run build:clean && npm run build",
    "test": "nyc --reporter=text mocha",
    "test:junit": "npm run test -- --reporter mocha-junit-reporter",
    "test:watch": "mocha --watch",
    "lint": "eslint --ext .ts --ext .tsx src/ test/",
    "lint:junit": "npm run lint -- --format junit --output-file test-lint.xml",
    "prestart": "npm run build",
    "start": "electron ./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaiSforza/durak.git"
  },
  "keywords": [
    "durak",
    "cards",
    "electron"
  ],
  "author": "Alex Giokas",
  "contributors": [
    {
      "name": "Kai Giokas",
      "email": "kai@kaictl.me",
      "url": "https://github.com/KaiSforza"
    },
    {
      "name": "Panda",
      "url": "https://github.com/pnda07"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaiSforza/durak/issues"
  },
  "homepage": "https://github.com/KaiSforza/durak#readme",
  "dependencies": {
    "electron": "^12.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.13.0",
    "fp-ts": "^2.8.5",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-loader": "^9.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "directories": {
    "src": "./src",
    "test": "./test"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "test/**/*.test.ts",
    "require": "ts-node/register",
    "ui": "tdd"
  }
}
